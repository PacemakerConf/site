<p id="notice"><%= notice %></p>

<h1>Listing Speakers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @speakers.each do |speaker| %>
      <tr>
        <td><%= speaker.name %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>
