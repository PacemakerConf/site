<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @contact.name %>
</p>

<p>
  <strong>Surname:</strong>
  <%= @contact.surname %>
</p>

<p>
  <strong>Telephone:<br></strong>
  <%= @contact.telephone %><br>
</p>

<p>
  <strong>Email:</strong><br>
  <td><% @contact.splited_mail.each do |mail| %>
             <%= mail %><br>
            <% end %></td>
</p>

<p>
  <strong>Skype:</strong>
  <%= @contact.skype %>
</p>

        


<%= link_to 'Edit', edit_admin_contact_path(@contact) %> |
